<% if notice %> 
 <p id= "notice" > <%= notice %> </p> 
 <% end %> 
 <div id="example" class= "tiles"> 
 <% @experiences.each do |experience| %> 
		 <%= link_to '#' do %>hiii<%= image_tag(experience.image_url) %>
     <% end %> 
 <% end %>
 </div>
 
<div class="center jumbotron">

 <h2><%= request.location.address %></h2>

<%= form_tag("/step_distance/index", method: :get, :class => "form-horizontal") do %>
<div class="form-group">
  <%= text_field_tag :city_name, nil, :placeholder => request.location.city %>
  <%= submit_tag 'Find experiences', class: "btn btn-lg btn-primary" %>
  </div>
<% end %>
</div>

